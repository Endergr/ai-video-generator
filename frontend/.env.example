# Backend API URL
REACT_APP_BACKEND_URL=http://localhost:8001

# Stripe Configuration
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here

# App Configuration
REACT_APP_MAX_FILE_SIZE=50MB
REACT_APP_SUPPORTED_FORMATS=jpg,jpeg,png,gif,webp

# Feature Flags
REACT_APP_ENABLE_NSFW=true
REACT_APP_ENABLE_PREMIUM=true
REACT_APP_ENABLE_SOCIAL_SHARING=true

# Analytics (Optional)
REACT_APP_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
REACT_APP_MIXPANEL_TOKEN=your_mixpanel_token

# Social Media Integration (Optional)
REACT_APP_TWITTER_API_KEY=your_twitter_api_key
REACT_APP_FACEBOOK_APP_ID=your_facebook_app_id

# Environment
REACT_APP_ENV=development
REACT_APP_DEBUG=true